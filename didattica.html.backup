    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        AOS.init();
        
        // Filter courses based on URL parameter
        document.addEventListener("DOMContentLoaded", function () {
            const urlParams = new URLSearchParams(window.location.search);
            const corso = urlParams.get('corso');
            
            if (corso && corso !== 'altro') {
                // Map old course names to new categories
                let category = corso;
                
                // Handle mapping for old parameter names to medicina
                if (corso === 'starter' || corso === 'intensive' || corso === 'premium' || corso === 'training') {
                    category = 'medicina';
                }
                
                // Get all course cards
                const allCards = document.querySelectorAll('[data-category]');
                const courseRow = document.querySelector('.row-equal-height');
                
                // Hide all cards first
                allCards.forEach(card => {
                    card.style.display = 'none';
                });
                
                // Show only cards matching the selected category
                const selectedCards = document.querySelectorAll(`[data-category="${category}"]`);
                selectedCards.forEach(card => {
                    card.style.display = 'flex';
                });
                
                // Center single cards
                if (selectedCards.length === 1) {
                    courseRow.classList.add('centered-cards');
                    selectedCards[0].classList.remove('col-lg-4');
                    selectedCards[0].classList.add('col-lg-6', 'mx-auto');
                } else {
                    courseRow.classList.remove('centered-cards');
                    // Reset all cards to original size
                    allCards.forEach(card => {
                        card.classList.remove('col-lg-6', 'mx-auto');
                        if (!card.classList.contains('col-lg-4')) {
                            card.classList.add('col-lg-4');
                        }
                    });
                }
                
                // Update page title
                const sectionTitle = document.querySelector('.section-title h1');
                const sectionSubtitle = document.querySelector('.section-title p');
                
                if (category === 'medicina') {
                    sectionTitle.textContent = 'Medicina, Odontoiatria e Veterinaria';
                    sectionSubtitle.textContent = 'Corsi preparatori per l\'ammissione alle facoltÃ  mediche';
                } else if (category === 'imat') {
                    sectionTitle.textContent = 'IMAT';
                    sectionSubtitle.textContent = 'Preparazione per l\'ammissione in lingua inglese';
                } else if (category === 'snt') {
                    sectionTitle.textContent = 'Professioni Sanitarie';
                    sectionSubtitle.textContent = 'Corsi per l\'ammissione alle professioni sanitarie';
                } else if (category === 'arched') {
                    sectionTitle.textContent = 'Architettura';
                    sectionSubtitle.textContent = 'Preparazione per l\'ammissione ad Architettura';
                }
                
                // Smooth scroll to top after a short delay
                setTimeout(function() {
                    window.scrollTo({top: 0, behavior: 'smooth'});
                }, 100);
            }
        });
    </script>
